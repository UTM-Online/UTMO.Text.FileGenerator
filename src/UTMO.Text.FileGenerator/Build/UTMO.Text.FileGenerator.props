<Project>
    
    <PropertyGroup Condition="'$(EnvName)' == ''">
        <EnvName>Default</EnvName>
    </PropertyGroup>
    
    <PropertyGroup>
        <_defaultOutputPath>$(MSBuildThisFileDirectory)bin\$(Configuration)\net6.0\FileGeneratorOut\$(EnvName)</_defaultOutputPath>
        <DisplayGeneratedOutput>false</DisplayGeneratedOutput>
        <GeneratedOutputVDirName>GeneratedOutput</GeneratedOutputVDirName>
        <AdditionalTemplateFiles/>
    </PropertyGroup>
    
    <PropertyGroup Condition="'$(IntermidateOutputDirectory)' == ''">
        <IntermidateOutputDirectory>$(_defaultOutputPath)</IntermidateOutputDirectory>
    </PropertyGroup>
    
</Project>